{
  "name": "workshop-repairs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "dotenv -- turbo build",
    "clean": "turbo clean && rm -rf .turbo node_modules .vercel",
    "db:export": "dotenv -- pnpm -F db db:export",
    "db:push": "dotenv -- pnpm -F db db:push",
    "db:reset": "dotenv -- pnpm -F db db:reset",
    "db:seed": "dotenv -- pnpm -F db db:seed",
    "db:studio": "dotenv -- pnpm -F db db:studio",
    "dev": "dotenv -- pnpm -F nextjs dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "start": "dotenv -- turbo start",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@turbo/gen": "^2.1.1",
    "@types/aws-lambda": "8.10.145",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.9.1",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sst": "^3.2.48",
    "turbo": "^2.1.1",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228"
