{
  "name": "workshop-repairs",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo build",
    "clean": "turbo clean && rm -rf .turbo node_modules .vercel",
    "db:export": "dotenv -- pnpm -F db db:export",
    "db:push": "dotenv -- pnpm -F db db:push",
    "db:reset": "dotenv -- pnpm -F db db:reset",
    "db:seed": "dotenv -- pnpm -F db db:seed",
    "db:studio": "dotenv -- pnpm -F db db:studio",
    "dev": "dotenv -- pnpm -F nextjs dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gen": "turbo gen workspace",
    "lint": "turbo lint --continue",
    "start": "dotenv -- turbo start",
    "typecheck": "turbo typecheck --continue",
    "v": "pnpm -v"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@turbo/gen": "^2.1.1",
    "@types/aws-lambda": "8.10.145",
    "commitizen": "^4.3.1",
    "commitlint": "^19.5.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.9.1",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "turbo": "^2.1.1",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
