{
  "name": "workshop-repairs",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo build",
    "clean": "turbo clean && rm -rf .turbo node_modules .vercel",
    "db:export": "dotenv -- pnpm -F db db:export",
    "db:push": "dotenv -- pnpm -F db db:push",
    "db:reset": "dotenv -- pnpm -F db db:reset",
    "db:seed": "dotenv -- pnpm -F db db:seed",
    "db:studio": "dotenv -- pnpm -F db db:studio",
    "dev": "dotenv -- pnpm -F nextjs dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gen": "turbo gen workspace",
    "lint": "turbo lint --continue",
    "start": "dotenv -- turbo start",
    "typecheck": "turbo typecheck --continue",
    "v": "pnpm -v"
  },
  "devDependencies": {
    "@turbo/gen": "^2.0.12",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.8.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "^2.0.12",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.7.0+sha512.dc09430156b427f5ecfc79888899e1c39d2d690f004be70e05230b72cb173d96839587545d09429b55ac3c429c801b4dc3c0e002f653830a420fa2dd4e3cf9cf"
}
